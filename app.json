{
  "expo": {
    "name": "PittGrub",
    "description": "Notifications for free food at the University of Pittsburgh.",
    "version": "1.0.1",
    "slug": "PittGrub",
    "sdkVersion": "24.0.0",
    "privacy": "unlisted",
    "platforms": ["ios"],
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "primaryColor": "#4C6AA4",
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
        "bundleIdentifier": "edu.pitt.cs.admt.pittgrub",
        "buildNumber": "1.0.1",
        "supportsTablet": false
    },
    "android": {
        "package": "edu.pitt.cs.admt.pittgrub",
        "versionCode": 1,
        "permissions": [
            "ACCESS_COARSE_LOCATION",
            "ACCESS_FINE_LOCATION",
            "CAMERA"
        ]
    }
  }
}
